; Wait for x us

WAIT100:
	PUSH	ACC
	MOV	A, #45
	DJNZ	ACC, $
	NOP
	POP	ACC
	RET

WAIT500:
	PUSH	ACC
	MOV	A, #245
	DJNZ	ACC, $
	NOP
	POP	ACC
	RET

WAIT1000:
	PUSH	ACC
	MOV	A, #239
	DJNZ	ACC, $
	DJNZ	ACC, $
	NOP
	POP	ACC
	RET

WAIT5000:
	PUSH	ACC
	CALL	WAIT1000
	CALL	WAIT1000
	CALL	WAIT1000
	CALL	WAIT1000
	MOV	A, #239
	DJNZ	ACC, $
	DJNZ	ACC, $
	NOP
	POP	ACC
	RET